{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "builds": [
    { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
    ]
  }

